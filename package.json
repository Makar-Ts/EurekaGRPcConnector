{
  "name": "nestjs-eureka-grpc-connector",
  "version": "0.2.1",
  "description": "A NestJS module for seamless integration between Eureka service discovery and gRPC clients with automatic service registration and load balancing",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "node dist/test.js",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist"
  },
  "author": {
    "name": "Makar Ts",
    "url": "https://github.com/Makar-Ts"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Makar-Ts/EurekaGRPcConnector"
  },
  "homepage": "https://github.com/Makar-Ts/EurekaGRPcConnector#readme",
  "bugs": {
    "url": "https://github.com/Makar-Ts/EurekaGRPcConnector/issues"
  },
  "keywords": [
    "nestjs",
    "grpc",
    "eureka",
    "microservices",
    "service-discovery",
    "load-balancing",
    "typescript",
    "netflix-eureka"
  ],
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.14.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@types/xml2js": "^0.4.14",
    "axios": "^1.12.2",
    "nestjs": "^0.0.1",
    "xml2js": "^0.6.2"
  }
}